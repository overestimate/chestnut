<interface>
  <requires lib="gtk" version="4.6"/>
  <template class="clickertest" parent="GtkWindow">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="clicker_label">
            <property name="halign">start</property>
            <property name="label">You clicked 0 times.</property>
          </object>
        </child>
        <child>
          <object class="GtkButton">
            <property name="label">Click</property>
            <signal name="clicked" handler="a_callback"/>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
