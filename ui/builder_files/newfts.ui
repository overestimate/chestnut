<interface>
  <requires lib="gtk" version="4.6"/>
  <template class="firsttimesetup" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkInfoBar">
        <property name="accessible-role">dialog</property>
        <property name="show-close-button">True</property>
        <property name="valign">baseline</property>
        <signal name="close" handler="close_infobar"/>
        <child>
          <object class="GtkLabel">
            <property name="label">If you know what you're doing, you can close this window.</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkButton" id="next_btn">
        <property name="halign">end</property>
        <property name="label">meowmeowmeow</property>
        <property name="margin-bottom">4</property>
        <property name="margin-end">6</property>
        <property name="margin-start">6</property>
        <property name="margin-top">4</property>
        <property name="name">next_btn</property>
        <property name="valign">end</property>
        <property name="vexpand">True</property>
        <signal name="clicked" handler="a_callback"/>
      </object>
    </child>
  </template>
</interface>
